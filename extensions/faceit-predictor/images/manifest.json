{
   "action": {
      "default_icon": {
         "128": "images/icon-128.png",
         "16": "images/icon-16.png",
         "32": "images/icon-32.png",
         "48": "images/icon-48.png"
      },
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "scripts/background.bundle.js"
   },
   "content_scripts": [ {
      "css": [ "scripts/content.css" ],
      "js": [ "scripts/lib/lz-string.js", "scripts/content.bundle.js" ],
      "matches": [ "https://www.faceit.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "Machine learning based match outcome predictor for CS2 matches on FACEIT",
   "host_permissions": [ "*://*.faceit.com/*" ],
   "icons": {
      "128": "images/icon-128.png",
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "FACEIT CS2 Predictor",
   "permissions": [ "webRequest", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.0.24",
   "web_accessible_resources": [ {
      "matches": [ "https://www.faceit.com/*" ],
      "resources": [ "scripts/content.css" ]
   } ]
}
